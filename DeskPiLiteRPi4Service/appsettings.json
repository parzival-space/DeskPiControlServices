{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "PowerManagement": {
    "SerialPort": "/dev/ttyUSB0",
    "BaudRate": 9600,
    "SyncCommand": "sync",
    "ShutdownCommand": "shutdown -h now"
  },
  "FanControl": {
    "FanPin": 14,
    "MinimumTime": 60,
    "FanProfile": {
      "0": 0.0,
      "40": 0.7,
      "45": 0.85,
      "50": 1.0
    }
  }
}
